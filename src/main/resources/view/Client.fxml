<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="ClientAncorpane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.Contollers.ClientController">
   <children>
      <VBox fx:id="VBOX" prefHeight="400.0" prefWidth="600.0">
         <children>
            <TextArea fx:id="txtArea" prefHeight="400.0" prefWidth="600.0" />
            <HBox fx:id="HBOX" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="txtMsg" prefHeight="39.0" prefWidth="394.0" />
                  <Button style="-fx-background-color: black;" fx:id="btnImage" mnemonicParsing="false" onAction="#btnImageOnAction" prefHeight="35.0" prefWidth="112.0" text="Image" />
                  <Button style="-fx-background-color: black" fx:id="btnSend" mnemonicParsing="false" onAction="#btnSendOnAction" prefHeight="35.0" prefWidth="112.0" text="Send" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
